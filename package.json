  "dependencies": {
    "@hotwired/stimulus": "^3.2.2",
    "@hotwired/turbo-rails": "^7.3.0",
    "autoprefixer": "^9",
    "esbuild": "^0.19.5",
    "sass": "^1.69.7",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat"
