<div id="<%= dom_id urgencium %>">
  <p>
    <strong>Tipo de urgencia:</strong>
    <%= urgencium.name %>
  </p>

  <%= link_to "Editar urgencia", edit_urgencium_path(@urgencium), class: "btn btn-info btn-sm" %> |
  <%= link_to "Volver a lista urgencias", urgencia_path, class: "btn btn-outline-primary btn-sm" %>
  <br><br>
  <%= button_to "Eliminar esta urgencia", @urgencium, method: :delete, class: "btn btn-danger btn-sm" %>

</div>
