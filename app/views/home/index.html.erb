<div id="logo">
  <!-- Aquí coloca tu logo o imagen -->
  <img src="/assets/logo.png" alt="Logo de la Clínica Dental">
</div>

<h1>Clinica dental</h1>

<% if user_signed_in? %>
  <div id="banner">
    Welcome <%= current_user.email %>
    <%= link_to "Log out", destroy_user_session_path, :class => "dropdown-item", data: { turbo: true, turbo_method: :delete} %>

  </div>
<% else %>
  <%= button_to "Sign in", new_user_session_path %>
<% end %>

<div id="whatsapp-button" class="whatsapp-button">
  <%= render 'whats_app_button' %>
</div>